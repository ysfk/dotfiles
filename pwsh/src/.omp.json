						"display_id": false,
						"display_name": true,
						"info_separator": " @ ",
						"prefix": " \uFD03 "
					}
				},
				{
					"type": "executiontime",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#ffffff",
					"background": "#8800dd",
