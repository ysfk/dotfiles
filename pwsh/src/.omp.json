{
	"final_space": false,
	"osc99": false,
	"console_title": true,
	"console_title_style": "template",
	"console_title_template": "{{if .Root}} ‚ö° {{end}}{{.Folder | replace \"~\" \"üèö\" }}",
	"terminal_background": "",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"horizontal_offset": 0,
			"vertical_offset": 0,
			"segments": [
				{
					"type": "root",
					"style": "plain",
					"foreground": "#FFEB3B",
					"background": "#a313a8",
					"properties": {
						"root_icon": "‚ö°"
					}
				},
				{
					"type": "os",
					"style": "plain",
					"foreground": "#ffffff",
					"background": "transparent"
				},
				{
					"type": "path",
					"style": "diamond",
					"powerline_symbol": "ÓÇ∞",
					"invert_powerline": false,
					"foreground": "#ffffff",
					"foreground_templates": null,
					"background": "#ff479c",
					"background_templates": null,
					"leading_diamond": "<transparent,#ff479c>\uE0B0</>",
					"trailing_diamond": "\uE0B0",
					"properties": {
						"enable_hyperlink": true,
						"prefix": " üìÅ ",
						"style": "folder",
						"mapped_locations": []
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"invert_powerline": false,
					"foreground": "#193549",
					"foreground_templates": null,
					"background": "#fffb38",
					"background_templates": null,
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"display_stash_count": true,
						"display_upstream_icon": true
					}
				},
				{
					"type": "node",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"invert_powerline": false,
					"foreground": "#ffffff",
					"foreground_templates": null,
					"background": "#6CA35E",
					"background_templates": null,
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"prefix": " \uE718 "
					}
				},
				{
					"type": "dotnet",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#000000",
					"background": "#00ffff",
					"properties": {
						"prefix": " \uE77F "
					}
				},
				{
					"type": "az",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#000000",
					"background": "#9ec3f0",
